<view class="commentall" wx:if="{{none}}">
  <image src="/assets/images/commentall.png"></image>
</view>
<view  wx:for="{{dishlist}}" >
  <view class="food_item">
    <navigator url="../details/details?id={{item.dishId}}">
      <image class="food_image" src="{{'https://shufood-1252832257.cos.ap-shanghai.myqcloud.com/'+item.dishImg}}"></image>
      <view class="food_info">

        <view class="uppart">
          <view class="foodname">{{item.dishName}}</view>
          <view class='food_canteen_box'>
            <text class="food_canteen">{{item.dishPlace}}{{item.dishFloor==1?'一楼  ':item.dishFloor==2?'二楼':'三楼'}}</text>
          </view>
        </view>
        <view class="downpart">
          <text class="comment">{{item.content}}</text>
        </view>
      </view>
    </navigator>
  </view>
</view>